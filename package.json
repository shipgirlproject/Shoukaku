{
  "name": "shoukaku",
  "version": "3.0.0-dev",
  "description": "A lavalink wrapper that supports almost all libraries",
  "main": "index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "compile": "tsc",
    "build": "npm run compile && npm run build:docs",
    "build:docs": "typedoc --theme default --readme README.md --out docs/ --entryPointStrategy expand src/."
  },
  "keywords": [
    "bot",
    "music",
    "lavalink",
    "api",
    "discord",
    "lavalink.js",
    "discord.js",
    "lavalink-api",
    "weeb-library"
  ],
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=7.0.0"
  },
  "author": "Saya",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Deivu/Shoukaku"
  },
  "dependencies": {
    "abort-controller": "^3.0.0",
    "petitio": "^1.4.0",
    "ws": "^8.5.0"
  },
  "devDependencies": {
    "@augu/eslint-config": "^2.2.0",
    "@types/node": "^17.0.35",
    "@types/ws": "^8.5.3",
    "@typescript-eslint/eslint-plugin": "^5.25.0",
    "@typescript-eslint/parser": "^5.25.0",
    "eslint": "^8.16.0",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.15",
    "typescript": "^4.6.4"
  }
}
